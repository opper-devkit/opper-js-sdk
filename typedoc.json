{
  "$schema": "https://typedoc.org/schema.json",
  "name": "opper",
  "out": "dist/docs",
  "entryPoints": [
    "packages/*"
  ],
  "entryPointStrategy": "packages",
  "exclude": [
    "**/*.(test|spec|d).ts"
  ],
  "excludeExternals": true,
  "excludeInternal": true,
  "githubPages": true,
  "readme": "./README.md",
  "categorizeByGroup": false
}
